{
  "name": "react-blog",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check": "biome check {src,tests}/**/*.{js,jsx,ts,tsx,html,css,less,sass,scss}",
    "check:ts": "tsc --noEmit",
    "check:all": "yarn check:ts && yarn check",
    "fix": "biome check --write {src,tests}/**/*.{js,jsx,ts,tsx,html,css,less,sass,scss}",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "sb": "storybook dev -p 6006 --no-open",
    "sb:build": "storybook build",
    "sb:test": "test-storybook",
    "sb:test:watch": "test-storybook --watch",
    "sb:test:detail": "test-storybook --verbose",
    "build-storybook": "storybook build",
    "lint-staged": "lint-staged",
    "chromatic": "chromatic",
    "prepare": "npx simple-git-hooks || :",
    "preinstall": "npx typesync || :",
    "postinstall": "lefthook install"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@react-buddy/ide-toolbox": "^2.4.0",
    "@react-buddy/palette-chakra-ui": "^2.3.1",
    "@storybook/test-runner": "^0.22.0",
    "@tanstack/react-query": "^5.45.0",
    "axios": "^1.7.2",
    "framer-motion": "^11.0.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-icons": "^5.0.1",
    "react-masonry-css": "^1.0.16",
    "react-router-dom": "^6.21.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@chromatic-com/storybook": "^3",
    "@playwright/test": "^1.52.0",
    "@storybook/addon-a11y": "8.6.14",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-interactions": "8.6.14",
    "@storybook/addon-links": "8.6.14",
    "@storybook/addon-onboarding": "8.6.14",
    "@storybook/blocks": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/react-vite": "8.6.14",
    "@storybook/test": "8.6.14",
    "@tanstack/react-query-devtools": "^5.45.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.6",
    "@types/react": "18.3",
    "@types/react-dom": "18.3",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "axe-playwright": "^2.0.1",
    "chromatic": "^10.5.2",
    "jsdom": "^24.0.0",
    "lefthook": "^1.11.13",
    "msw": "^2.1.5",
    "msw-storybook-addon": "2.0.0--canary.122.b3ed3b1.0",
    "playwright": "^1.52.0",
    "storybook": "8.6.14",
    "storybook-addon-remix-react-router": "^3.0.0",
    "typescript": "*",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4"
  },
  "packageManager": "yarn@4.0.2",
  "msw": {
    "workerDirectory": ["public"]
  }
}
